.fundetected-window {
    background-color: #2d2c2c;
}

.fundetected-window-header {
    background-color: #320f06;
    border-bottom-width: 3px;
    border-bottom-color: #54514f;
    width: 100%;
    height: 35px;
}

.fundetected-window-title {
    color: #ffffff;
    font-size: 20px; /* 2 sizes bigger than default */
    -unity-font-style: bold;
    -unity-text-align: middle-center;
    align-self: center;
    flex-grow: 1;
}