<animations>
	<entryAnimation id="idle" speedMultiplier="1.0">
		<animationFrames spriteImageName="AnimatedThornBushIdle" isLooping="true">
			<animationFrame index="0"/>
			<animationFrame index="1"/>
			<animationFrame index="2"/>
		</animationFrames>
		<transitions>
			<transition target="attack" />
		</transitions>
	</entryAnimation>
	<animation id="attack" speedMultiplier="1.0">
		<animationFrames spriteImageName="AnimatedThornBushAttack" isLooping="false">
			<animationFrame index="0"/>
			<animationFrame index="1"/>
		</animationFrames>
		<transitions>
			<transition target="entry" />
			<transition target="attack" />
		</transitions>
	</entryAnimation>
</animations>